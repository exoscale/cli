_exo_api_vm()
{
    last_command="exo_api_vm"

    command_aliases=()

    commands=()
    commands+=("addNic")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("addNicToVirtualMachine")
        aliashash["addNicToVirtualMachine"]="addNic"
    fi
    commands+=("changeService")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("changeServiceForVirtualMachine")
        aliashash["changeServiceForVirtualMachine"]="changeService"
    fi
    commands+=("deleteReverseDNSFromVM")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("deleteReverseDnsFromVirtualMachine")
        aliashash["deleteReverseDnsFromVirtualMachine"]="deleteReverseDNSFromVM"
    fi
    commands+=("deploy")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("deployVirtualMachine")
        aliashash["deployVirtualMachine"]="deploy"
    fi
    commands+=("destroy")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("destroyVirtualMachine")
        aliashash["destroyVirtualMachine"]="destroy"
    fi
    commands+=("expunge")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("expungeVirtualMachine")
        aliashash["expungeVirtualMachine"]="expunge"
    fi
    commands+=("getPassword")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("getVMPassword")
        aliashash["getVMPassword"]="getPassword"
    fi
    commands+=("getUserData")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("getVirtualMachineUserData")
        aliashash["getVirtualMachineUserData"]="getUserData"
    fi
    commands+=("list")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("listVirtualMachines")
        aliashash["listVirtualMachines"]="list"
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("migrateVirtualMachine")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("migrateVirtualMachine")
        aliashash["migrateVirtualMachine"]="migrateVirtualMachine"
    fi
    commands+=("queryReverseDNSForVM")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("queryReverseDnsForVirtualMachine")
        aliashash["queryReverseDnsForVirtualMachine"]="queryReverseDNSForVM"
    fi
    commands+=("reboot")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("rebootVirtualMachine")
        aliashash["rebootVirtualMachine"]="reboot"
    fi
    commands+=("recover")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("recoverVirtualMachine")
        aliashash["recoverVirtualMachine"]="recover"
    fi
    commands+=("removeNic")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("removeNicFromVirtualMachine")
        aliashash["removeNicFromVirtualMachine"]="removeNic"
    fi
    commands+=("resetPassword")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("resetPasswordForVirtualMachine")
        aliashash["resetPasswordForVirtualMachine"]="resetPassword"
    fi
    commands+=("restore")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("restoreVirtualMachine")
        aliashash["restoreVirtualMachine"]="restore"
    fi
    commands+=("scale")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("scaleVirtualMachine")
        aliashash["scaleVirtualMachine"]="scale"
    fi
    commands+=("start")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("startVirtualMachine")
        aliashash["startVirtualMachine"]="start"
    fi
    commands+=("stop")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("stopVirtualMachine")
        aliashash["stopVirtualMachine"]="stop"
    fi
    commands+=("update")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateVirtualMachine")
        aliashash["updateVirtualMachine"]="update"
    fi
    commands+=("updateReverseDNSForVM")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateReverseDnsForVirtualMachine")
        aliashash["updateReverseDnsForVirtualMachine"]="updateReverseDNSForVM"
    fi
    commands+=("updateVMAffinityGroup")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateVMAffinityGroup")
        aliashash["updateVMAffinityGroup"]="updateVMAffinityGroup"
    fi
    commands+=("updateVMNicIP")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateVmNicIp")
        aliashash["updateVmNicIp"]="updateVMNicIP"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("-C")
    flags+=("--debug")
    flags+=("-d")
    flags+=("--use-account=")
    two_word_flags+=("-A")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

