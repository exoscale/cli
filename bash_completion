_exo_vm()
{
    last_command="exo_vm"

    command_aliases=()

    commands=()
    commands+=("create")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("add")
        aliashash["add"]="create"
    fi
    commands+=("delete")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("del")
        aliashash["del"]="delete"
    fi
    commands+=("list")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("reboot")
    commands+=("reset")
    commands+=("resize")
    commands+=("scale")
    commands+=("serviceoffering")
    commands+=("show")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("get")
        aliashash["get"]="show"
    fi
    commands+=("start")
    commands+=("stop")
    commands+=("template")
    commands+=("updateip")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("-C")
    flags+=("--use-account=")
    two_word_flags+=("-A")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

