_exo_api()
{
    last_command="exo_api"

    command_aliases=()

    commands=()
    commands+=("account")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("acc")
        aliashash["acc"]="account"
    fi
    commands+=("address")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("addr")
        aliashash["addr"]="address"
    fi
    commands+=("affinity-group")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("ag")
        aliashash["ag"]="affinity-group"
    fi
    commands+=("api")
    commands+=("async-job")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("aj")
        aliashash["aj"]="async-job"
    fi
    commands+=("event")
    commands+=("network")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("net")
        aliashash["net"]="network"
    fi
    commands+=("nic")
    commands+=("offering")
    commands+=("security-group")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("sg")
        aliashash["sg"]="security-group"
    fi
    commands+=("snapshot")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("snap")
        aliashash["snap"]="snapshot"
    fi
    commands+=("ssh")
    commands+=("tag")
    commands+=("template")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("temp")
        aliashash["temp"]="template"
    fi
    commands+=("user")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("usr")
        aliashash["usr"]="user"
    fi
    commands+=("vm")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("virtual-machine")
        aliashash["virtual-machine"]="vm"
    fi
    commands+=("vm-group")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("vg")
        aliashash["vg"]="vm-group"
    fi
    commands+=("volume")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("vol")
        aliashash["vol"]="volume"
    fi
    commands+=("zone")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--debug")
    flags+=("-d")
    flags+=("--config=")
    two_word_flags+=("-C")
    flags+=("--use-account=")
    two_word_flags+=("-A")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

