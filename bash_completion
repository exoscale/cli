_exo_api_address()
{
    last_command="exo_api_address"

    command_aliases=()

    commands=()
    commands+=("associate")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("assoc")
        aliashash["assoc"]="associate"
        command_aliases+=("associateIpAddress")
        aliashash["associateIpAddress"]="associate"
    fi
    commands+=("deleteReverseDNSFromAddress")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("deleteReverseDnsFromPublicIpAddress")
        aliashash["deleteReverseDnsFromPublicIpAddress"]="deleteReverseDNSFromAddress"
    fi
    commands+=("disassociate")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("disassociate")
        aliashash["disassociate"]="disassociate"
        command_aliases+=("disassociateIpAddress")
        aliashash["disassociateIpAddress"]="disassociate"
        command_aliases+=("dissoc")
        aliashash["dissoc"]="disassociate"
    fi
    commands+=("list")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("listPublicIpAddresses")
        aliashash["listPublicIpAddresses"]="list"
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("queryReverseDNSForAddress")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("queryReverseDnsForPublicIpAddress")
        aliashash["queryReverseDnsForPublicIpAddress"]="queryReverseDNSForAddress"
    fi
    commands+=("update")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateIpAddress")
        aliashash["updateIpAddress"]="update"
    fi
    commands+=("updateReverseDNSForAddress")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("updateReverseDnsForPublicIpAddress")
        aliashash["updateReverseDnsForPublicIpAddress"]="updateReverseDNSForAddress"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("-C")
    flags+=("--debug")
    flags+=("-d")
    flags+=("--use-account=")
    two_word_flags+=("-A")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

