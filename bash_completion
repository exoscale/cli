_exo_api_ssh()
{
    last_command="exo_api_ssh"

    command_aliases=()

    commands=()
    commands+=("create")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("add")
        aliashash["add"]="create"
        command_aliases+=("createSSHKeyPair")
        aliashash["createSSHKeyPair"]="create"
    fi
    commands+=("delete")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("del")
        aliashash["del"]="delete"
        command_aliases+=("deleteSSHKeyPair")
        aliashash["deleteSSHKeyPair"]="delete"
    fi
    commands+=("list")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("listSSHKeyPairs")
        aliashash["listSSHKeyPairs"]="list"
        command_aliases+=("ls")
        aliashash["ls"]="list"
    fi
    commands+=("register")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("registerSSHKeyPair")
        aliashash["registerSSHKeyPair"]="register"
    fi
    commands+=("reset")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("resetSSHKeyForVirtualMachine")
        aliashash["resetSSHKeyForVirtualMachine"]="reset"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("-C")
    flags+=("--debug")
    flags+=("-d")
    flags+=("--use-account=")
    two_word_flags+=("-A")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

